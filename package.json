{
  "private": true,
  "name": "spryker-b2c-demo-shop",
  "description": "Spryker B2C Demo Shop project",
  "author": {
    "name": "Spryker Systems GmbH",
    "email": "info@spryker.com",
    "url": "https://spryker.com/"
  },
  "scripts": {
    "yves": "run-s \"yves:development:* -- {@}\" --",
    "yves:development:esm": "node ./frontend/build development -m module:esm",
    "yves:development:legacy": "node ./frontend/build development -m module:legacy",
    "yves:watch": "run-p \"yves:watch:* -- {@}\" --",
    "yves:watch:esm": "node ./frontend/build development-watch -m module:esm",
    "yves:watch:legacy": "node ./frontend/build development-watch -m module:legacy",
    "yves:production": "run-s \"yves:production:* -- {@}\" --",
    "yves:production:esm": "node ./frontend/build production -m module:esm",
    "yves:production:legacy": "node ./frontend/build production -m module:legacy",
    "yves:help": "node ./frontend/libs/command-line-parser --help",
    "yves:stylelint": "NODE_ENV=development node ./frontend/libs/stylelint development",
    "yves:tslint": "node ./frontend/libs/tslint",
    "yves:image-optimization": "node ./frontend/libs/image-optimization",
    "welcome:build": "node ./frontend/welcome/builder/development",
    "welcome:watch": "node ./frontend/welcome/builder/development-watch",
    "welcome:build:production": "node ./frontend/welcome/builder/production",
    "zed": "node ./frontend/zed-build",
    "zed:watch": "node ./frontend/zed-build --dev",
    "zed:production": "node ./frontend/zed-build --prod",
    "storeapp": "run-s \"storeapp:development:* -- {@}\" --",
    "storeapp:development:esm": "node ./frontend/storeapp/build development -m module:esm",
    "storeapp:development:legacy": "node ./frontend/storeapp/build development -m module:legacy",
    "storeapp:watch": "run-p \"storeapp:watch:* -- {@}\" --",
    "storeapp:watch:esm": "node ./frontend/storeapp/build development-watch -m module:esm",
    "storeapp:watch:legacy": "node ./frontend/storeapp/build development-watch -m module:legacy",
    "storeapp:production": "run-s \"storeapp:production:* -- {@}\" --",
    "storeapp:production:esm": "node ./frontend/storeapp/build production -m module:esm",
    "storeapp:production:legacy": "node ./frontend/storeapp/build production -m module:legacy"
  },
  "engines": {
    "node": ">=8.11.4",
    "npm": ">=6.4.1"
  },
  "dependencies": {
    "@webcomponents/custom-elements": "~1.3.1",
    "@webcomponents/webcomponents-platform": "~1.0.1",
    "@webcomponents/webcomponentsjs": "~2.4.0",
    "classlist-polyfill": "~1.2.0",
    "core-js": "~3.4.1",
    "element-closest": "~3.0.0",
    "jquery": "~3.4.1",
    "jquery-datetimepicker": "~2.5.21",
    "lodash-es": "~4.17.14",
    "lozad": "^1.14.0",
    "nouislider": "~11.1.0",
    "ol": "^6.1.1",
    "select2": "~4.0.11",
    "slick-carousel": "~1.8.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@babel/plugin-proposal-class-properties": "~7.8.3",
    "@babel/plugin-transform-runtime": "~7.9.0",
    "@babel/preset-env": "~7.9.0",
    "@babel/preset-typescript": "~7.9.0",
    "@spryker/oryx-for-zed": "~2.5.2",
    "autoprefixer": "~8.6.2",
    "babel-loader": "~8.1.0",
    "clean-webpack-plugin": "~1.0.1",
    "commander": "~4.0.1",
    "copy-webpack-plugin": "~5.0.5",
    "css-loader": "~3.2.0",
    "fast-glob": "~3.1.0",
    "imagemin-keep-folder": "~5.3.2",
    "imagemin-pngquant": "~8.0.0",
    "imagemin-svgo": "~7.1.0",
    "jimp": "~0.10.0",
    "mini-css-extract-plugin": "~0.8.0",
    "node-sass": "~4.13.0",
    "npm-run-all": "~4.1.5",
    "optimize-css-assets-webpack-plugin": "~5.0.1",
    "postcss-loader": "~3.0.0",
    "sass-loader": "~8.0.0",
    "sass-resources-loader": "~2.0.1",
    "stylelint": "~12.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "tslint": "~5.20.1",
    "tslint-lines-between-class-members": "~1.3.6",
    "typescript": "~3.7.2",
    "typescript-eslint-parser": "~22.0.0",
    "webpack": "~4.41.2",
    "webpack-merge": "~4.2.2"
  }
}
