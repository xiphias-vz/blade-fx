{% extends template('page-layout-main') %}

{% block pageInfo %}{% endblock %}

{% block container %}

        {% embed organism('jumbotron-slider') only %}
            {% block body %}
                {% embed molecule('slick-carousel') with {
                    attributes: {
                        'data-json': attributes['data-json'],
                    },
                } only %}
                    {% block slides %}
                        {% cms_slot 'slt-2' %}
                    {% endblock %}
                {% endembed %}
            {% endblock %}
        {% endembed %}

        {% embed organism('jumbotron-slider-mobile') only %}
            {% block body %}
                {% embed molecule('slick-carousel') with {
                    attributes: {
                        'data-json': attributes['data-json'],
                    },
                } only %}
                    {% block slides %}
                        {% cms_slot 'slt-2-mobile' %}
                    {% endblock %}
                {% endembed %}
            {% endblock %}
        {% endembed %}
    <div class="navigation-category__container">
            {% embed organism('section') with {
                modifiers: ['secondary', 'full-width','homepage'],
                class: 'quick-links-section spacing-top spacing-top--inner spacing-top--big',
            } only %}
                {% block inner %}
                    <div class="quicklinks grid grid--left grid--middle">
                        {% cms_slot 'quick-links-slt' %}
                    </div>
                {% endblock %}
            {% endembed %}
            <div class="container__inner" style="background-color: white">
                {% block content %}
            </div>
            <div class="slot3-container">
                {% cms_slot 'slt-3' %}
            </div>
    </div>
            <div class="navigation-category__gray-container">
                <div class="navigation-category__container">
                    {{ content_navigation('navigation-categories', 'navigation-categories') }}
                    {% cms_slot 'slt-1st-in-frame' %}
                </div>
            </div>
               <div class="navigation-category__container">
                    {% embed organism('section') with {
                        modifiers: ['secondary', 'full-width','homepage'],
                        class: 'spacing-top spacing-top--inner spacing-top--big',
                    } only %}
                        {% block inner %}
                                <div class=" grid grid--left grid--middle">
                                    {% cms_slot '3-elements-slt-in-frame-grid-content' %}
                                </div>
               </div>
                        {% endblock %}
                    {% endembed %}
        <div class="navigation-category__container">
                    {% embed organism('section') with {
                        modifiers: ['secondary', 'full-width'],
                        class: 'spacing-top spacing-top--inner spacing-top--big',
                        } only %}
                        {% block inner %}
                            <div class="grid grid--left grid--middle">
                                {% cms_slot '4-elements-slt-in-frame-grid-content' %}
                            </div>
                        {% endblock %}
                    {% endembed %}
                {% endblock %}
                {% block downbar %}
                        {% cms_slot 'downbar-slt' %}
                {% endblock %}
        </div>
    </main>
{% endblock %}
