            data: {
                properties: {
                    component: 'url="https://globus-sb.fact-finder.de/fact-finder"
                        version="ng"
                        api="v4"
                        channel="' ~ data.channelName ~'"
                        default-query="*"
                        search-immediate="false"
                        use-url-parameters="false"
                        use-browser-history="true"
                        currency-fields="Price"'
                },
            }
        } only %}
    {% endif %}

