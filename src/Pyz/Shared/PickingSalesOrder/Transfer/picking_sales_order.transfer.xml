<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="PickingSalesOrder">
        <property name="idPickingSalesOrder" type="int"/>
        <property name="containerCode" type="string"/>
        <property name="shelfCode" type="string"/>
        <property name="idSalesOrder" type="int"/>
        <property name="idPickingZone" type="int"/>
        <property name="abbreviation" type="string"/>
    </transfer>

    <transfer name="PickingSalesOrderCollection">
        <property name="pickingSalesOrders" type="PickingSalesOrder[]" singular="PickingSalesOrder"/>
    </transfer>

    <transfer name="PickingSalesOrderCriteria">
        <property name="idSalesOrder" type="int"/>
        <property name="idPickingZone" type="int"/>
        <property name="containerCodes" type="string[]" singular="containerCode"/>
    </transfer>

</transfers>
