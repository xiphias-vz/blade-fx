<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ShipmentMethod">
        <property name="timeSlots" type="array"/>
    </transfer>

    <transfer name="WeekDayTimeSlots">
        <property name="weekDayName" type="string"/>
        <property name="timeSlotsCapacity" type="TimeSlotCapacity[]"/>
    </transfer>

    <transfer name="DateTimeSlots">
        <property name="Date" type="string"/>
        <property name="timeSlotsCapacity" type="TimeSlotCapacity[]"/>
    </transfer>

    <transfer name="TimeSlotCapacity">
        <property name="merchantReference" type="string"/>
        <property name="day" type="string"/>
        <property name="date" type="string"/>
        <property name="timeSlot" type="string"/>
        <property name="capacity" type="int"/>
    </transfer>

    <transfer name="Merchant">
        <property name="weekDaysTimeSlots" type="WeekDayTimeSlots[]"/>
        <property name="weekDaysTimeSlotsRaw" type="array"/>
        <property name="dateTimeSlots" type="DateTimeSlots[]"/>
        <property name="dateTimeSlotsRaw" type="array"/>
    </transfer>

    <transfer name="MerchantCriteriaFilter">
        <property name="withTimeSlots" type="bool"/>
    </transfer>

    <transfer name="MerchantCriteria">
        <property name="withTimeSlots" type="bool"/>
    </transfer>

</transfers>
