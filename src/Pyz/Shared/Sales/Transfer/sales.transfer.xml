<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Order">
        <property name="merchantReference" type="string"/>
        <property name="pickingBagsCount" type="int"/>
        <property name="isCancelled" type="bool" />
        <property name="status" type="string" />
        <property name="isCancellableByCustomer" type="bool" />
        <property name="statusDate" type="string" />
        <property name="invoiceReference" type="string" />
        <property name="invoiceCreatedAt" type="string" />
        <property name="fkUser" type="int"/>
        <property name="storeStatus" type="string" />
        <property name="deliveredAt" type="string" />
        <property name="pickedAt" type="string" />
        <property name="collectedAt" type="string" />
        <property name="merchantRegionNumber" type="int"/>
        <property name="merchantFilialNumber" type="int"/>
        <property name="merchantName" type="string"/>
        <property name="deliveryMinimumAge" type="int"/>
        <property name="requestedDeliveryDate" type="string"/>
        <property name="paybackNumber" type="string"/>
        <property name="isConnected" type="bool"/>
        <property name="pickingSalesOrderCollection" type="PickingSalesOrderCollection"/>
    </transfer>

    <transfer name="Item">
        <property name="productNumber" type="string"/>
        <property name="pickZone" type="string"/>
        <property name="assortmentZone" type="string"/>
        <property name="sapWgr" type="string"/>
        <property name="newWeight" type="int"/>
        <property name="bontext" type="string"/>
        <property name="weightPerUnit" type="int"/>
        <property name="pricePerKg" type="int"/>
        <property name="sequence" type="string"/>
        <property name="alternativeEan" type="string"/>
        <property name="shelf" type="string"/>
        <property name="shelfFloor" type="string"/>
        <property name="shelfField" type="string"/>
        <property name="brand" type="string"/>
        <property name="basePriceContent" type="int"/>
        <property name="basePriceUnit" type="string"/>
        <property name="itemPaused" type="bool" />
        <property name="colorId" type="int" />
        <property name="aisle" type="int" />
        <property name="pickedAt" type="string" />
        <property name="originalPrice" type="int" />
        <property name="discountedPriceFrom" type="datetime" />
        <property name="discountedPriceTo" type="datetime" />
    </transfer>

    <transfer name="ProductConcrete">
        <property name="productNumber" type="string"/>
    </transfer>

    <transfer name="OrderCriteriaFilter">
        <property name="merchantReferences" type="string[]" singular="merchantReference"/>
        <property name="assignedIdUsers" type="int[]" singular="assignedIdUser"/>
        <property name="assignedIdUserCanBeNull" type="bool"/>
        <property name="storeStatuses" type="string[]" singular="storeStatus"/>
        <property name="deliveryDate" type="string"/>
        <property name="shipmentName" type="string"/>
        <property name="requestedDeliveryDate" type="string"/>
        <property name="orderCountLimit" type="int"/>
        <property name="idSalesOrders" type="int[]" singular="idSalesOrder"/>
        <property name="excludeCancelledOrders" type="bool"/>
    </transfer>

    <transfer name="OrderUpdateRequest">
        <property name="fkUser" type="string"/>
        <property name="storeStatus" type="string"/>
        <property name="pickingBagsCount" type="int"/>
        <property name="pickedAt" type="string"/>
        <property name="collectedAt" type="string"/>
        <property name="deliveredAt" type="string"/>
    </transfer>

    <transfer name="OrderChangeRequest">
        <property name="fkSalesOrder" type="int"/>
        <property name="orderItemChangeRequest" type="OrderItemChangeRequest[]"/>
    </transfer>

    <transfer name="OrderItemChangeRequest">
        <property name="idSalesOrderItem" type="int"/>
        <property name="quantity" type="int"/>
        <property name="newWeight" type="int"/>
        <property name="price" type="int"/>
    </transfer>

</transfers>
