<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <!-- zanemari ovaj prvi za sad -->
    <transfer name="RecommendationDefinitionStorage">
        <property name="recommendationsStorage" type="string" />
    </transfer>

    <transfer name="RecommendationDefinitionCollection">
        <property name="definitions" type="RecommendationDefinition[]" />
        <property name="store" type="string" />
    </transfer>

    <transfer name="RecommendationDefinition">
        <property name="idRecommendationDefinition" type="int" />
        <property name="store" type="string" />
    </transfer>

    <transfer name="RecommendationDefinitionCriteria">
        <property name="idStore" type="int" />
        <property name="store" type="string" />
    </transfer>

</transfers>
