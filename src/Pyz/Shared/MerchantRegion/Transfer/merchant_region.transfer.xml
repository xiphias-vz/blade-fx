<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="MerchantRegion">
        <property name="abbreviation" type="string"/>
        <property name="regionNumber" type="int"/>
        <property name="regionName" type="string"/>
        <property name="street" type="string"/>
        <property name="zipCode" type="string"/>
        <property name="city" type="string"/>
        <property name="vat" type="string"/>
        <property name="fax" type="string"/>
        <property name="footerText" type="string"/>
    </transfer>

    <transfer name="Order">
        <property name="merchantRegion" type="MerchantRegion"/>
    </transfer>

    <transfer name="OrderDetailRequest">
        <property name="merchantRegion" type="MerchantRegion"/>
    </transfer>

    <transfer name="Customer">
        <property name="merchantRegion" type="MerchantRegion"/>
    </transfer>

</transfers>
