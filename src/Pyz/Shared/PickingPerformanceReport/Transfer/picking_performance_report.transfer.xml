<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="PerformanceSalesOrderReport">
        <property name="idPerformanceSalesOrderReport" type="int"/>
        <property name="idGlobalPickReport" type="int"/>
        <property name="idSalesOrder" type="int"/>
        <property name="orderDate" type="string"/>
        <property name="pickupDate" type="string"/>
        <property name="containersUsed" type="int"/>
        <property name="positionsUsed" type="int"/>
        <property name="pieces" type="int"/>
        <property name="pickingStart" type="string"/>
        <property name="pickingEnd" type="string"/>
        <property name="pickupStart" type="string"/>
        <property name="pickupEnd" type="string"/>
        <property name="durationPickingTime" type="int" />
    </transfer>

    <transfer name="PerformanceSalesOrderItemReport">
        <property name="idPerformanceSalesOrderItemReport" type="int"/>
        <property name="idPerformanceSalesOrderReport" type="int"/>
        <property name="idSalesOrderItem" type="int"/>
        <property name="pickingStartTime" type="string" />
        <property name="pickingEndTime" type="string" />
        <property name="pickupEndStatus" type="string" />
        <property name="durationPickingTime" type="int" />
    </transfer>

    <transfer name="PerformanceGlobalSalesOrderReport">
        <property name="idGlobalPickReport" type="int"/>
        <property name="idPicker" type="int"/>
        <property name="pickZone" type="string"/>
        <property name="isMultiPick" type="string" />
        <property name="numberRelatedOrders" type="string" />
        <property name="pickTimeBegin" type="string" />
        <property name="pickTimeEnd" type="string" />
        <property name="durationPickingTime" type="int" />
    </transfer>

</transfers>
