<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Quote">
        <property name="termsApproval" type="bool"/>
        <property name="IsSubstitutionAllowed" type="bool" />
        <property name="isDepositAllowed" type="bool" />
        <property name="quoteZoneMapping" type="QuoteZoneMapping[]" />
    </transfer>

    <transfer name="Customer">
        <property name="isAdvertise" type="bool"/>
        <property name="addressStatus" type="string"/>
        <property name="isScanAndGo" type="bool"/>
        <property name="isMeinGlobus" type="bool"/>
    </transfer>

    <transfer name="QuoteZoneMapping">
        <property name="merchantReference" type="string" />
        <property name="assortmentZone" type="string" />
        <property name="pickZone" type="string" />
    </transfer>

</transfers>
