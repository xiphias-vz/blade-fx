<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="StorePickingListCollection">
        <property name="storePickingLists" type="StorePickingList[]" singular="storePickingList" />
    </transfer>

    <transfer name="StorePickingList">
        <property name="merchantDetails" type="string"/>
        <property name="deliveryDate" type="string"/>
        <property name="orderReference" type="string"/>
        <property name="customerReference" type="string"/>
        <property name="items" type="StorePickingListItem[]"/>
    </transfer>

    <transfer name="StorePickingListItem">

        <property name="productNumber" type="string"/>
        <property name="productName" type="string"/>
        <property name="productVariety" type="string"/>
        <property name="quantity" type="int"/>
    </transfer>

</transfers>
