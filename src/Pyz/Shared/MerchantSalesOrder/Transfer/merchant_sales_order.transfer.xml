<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Item">
        <property name="productOffer" type="ProductOffer"/>
    </transfer>

    <transfer name="Order">
        <property name="merchantSalesOrder" type="MerchantSalesOrder"/>
    </transfer>

    <transfer name="MerchantSalesOrderCollection">
        <property name="merchantSalesOrders" type="MerchantSalesOrder[]" singular="merchantSalesOrder"/>
    </transfer>

    <transfer name="MerchantSalesOrder">
        <property name="fkMerchant" type="int"/>
        <property name="storeStatus" type="string"/>
        <property name="requestedDeliveryDate" type="string"/>
        <property name="fkUser" type="int"/>

        <property name="deliveredAt" type="string" />
        <property name="pickedAt" type="string" />
        <property name="collectedAt" type="string" />
    </transfer>

</transfers>
