<?xml version="1.0"?>
<database xmlns="spryker:schema-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="zed" namespace="Orm\Zed\Sales\Persistence" package="src.Orm.Zed.Sales.Persistence" xsi:schemaLocation="spryker:schema-01 https://static.spryker.com/schema-01.xsd">

    <table name="spy_sales_discount">
        <column name="id_sales_discount" required="true" type="BIGINT" autoIncrement="true" primaryKey="true"/>
        <column name="fk_sales_order" type="BIGINT" required="false"/>
        <column name="fk_sales_order_item" type="BIGINT" required="false"/>
        <column name="fk_sales_expense" type="BIGINT" required="false"/>
        <column name="fk_sales_order_item_option" type="BIGINT" required="false"/>
    </table>

    <table name="spy_sales_discount_code">
        <column name="id_sales_discount_code" required="true" type="BIGINT" autoIncrement="true" primaryKey="true"/>
        <column name="fk_sales_discount" type="BIGINT" required="true"/>
    </table>

</database>
