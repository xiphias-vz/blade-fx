<?xml version="1.0"?>
<database xmlns="spryker:schema-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="zed" xsi:schemaLocation="spryker:schema-01 https://static.spryker.com/schema-01.xsd"
          namespace="Orm\Zed\PostalCode\Persistence"
          package="src.Orm.Zed.PostalCode.Persistence">

    <table name="pyz_postal_code">
        <column name="id_postal_code" required="true" type="INTEGER" autoIncrement="true" primaryKey="true"/>
        <column name="postal_code" type="VARCHAR" required="true"/>

        <unique name="pyz_postal_code-unique-key">
            <unique-column name="postal_code"/>
        </unique>

        <id-method-parameter value="pyz_postal_code_pk_seq"/>
    </table>

</database>
