<?xml version="1.0"?>
<database xmlns="spryker:schema-01"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          name="zed"
          namespace="Orm\Zed\DataImport\Persistence"
          package="src.Orm.Zed.DataImport.Persistence"
          xsi:schemaLocation="spryker:schema-01 https://static.spryker.com/schema-01.xsd">

    <table name="pyz_data_import_event">
        <column name="id_data_import_event" required="true" type="BIGINT" autoIncrement="true" primaryKey="true"/>
        <column name="entity_id" type="INTEGER" required="true"/>
        <column name="event_name" type="VARCHAR" size="100"/>
        <column name="event_data" type="VARCHAR" size="1000"/>
        <column name="created_at" required="true" type="TIMESTAMP"/>
    </table>

    <table name="pyz_data_import_event_archive">
        <column name="id" required="true" type="BIGINT" autoIncrement="true" primaryKey="true"/>
        <column name="id_data_import_event" required="true" type="BIGINT"/>
        <column name="entity_id" type="INTEGER"/>
        <column name="event_name" type="VARCHAR" size="100"/>
        <column name="event_data" type="VARCHAR" size="1000"/>
        <column name="created_at" type="TIMESTAMP"/>
        <column name="executed_at" type="TIMESTAMP"/>
    </table>

</database>
