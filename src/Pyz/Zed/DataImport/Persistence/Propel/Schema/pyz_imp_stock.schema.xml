<?xml version="1.0"?>
<database xmlns="spryker:schema-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="zed" xsi:schemaLocation="spryker:schema-01 https://static.spryker.com/schema-01.xsd" namespace="Orm\Zed\DataImport\Persistence" package="src.Orm.Zed.DataImport.Persistence">

    <table name="pyz_imp_stock">
        <column name="id" required="true" type="BIGINT" autoIncrement="true" primaryKey="true"/>
        <column name="store" type="VARCHAR" size="20" required="true"/>
        <column name="sapnumber" type="VARCHAR" size="50"/>
        <column name="gtin" type="VARCHAR" size="50"/>
        <column name="instock" type="DECIMAL" size="20" scale="6"/>
        <column name="shelf" type="VARCHAR" size="50"/>
        <column name="shelffield" type="VARCHAR" size="50"/>
        <column name="shelffloor" type="VARCHAR" size="50"/>
        <column name="filename" type="VARCHAR" size="200"/>
        <column name="dtImported" type="TIMESTAMP"/>

        <index name="pyz_imp_stock-index_store">
            <index-column name="store"/>
        </index>

    </table>

</database>
