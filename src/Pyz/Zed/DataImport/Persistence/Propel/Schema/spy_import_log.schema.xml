<?xml version="1.0"?>
<database xmlns="spryker:schema-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="zed" xsi:schemaLocation="spryker:schema-01 https://static.spryker.com/schema-01.xsd" namespace="Orm\Zed\Import\Persistence" package="src.Orm.Zed.Import.Persistence">

    <table name="spy_import_log" phpName="SpyImportLog">
        <column name="id_log" required="true" type="INTEGER" primaryKey="true" autoIncrement="true" />
        <column name="import_name" type="VARCHAR" size="100" required="true"/>
        <column name="import_desc" type="VARCHAR" size="500"/>
        <column name="log_text" type="VARCHAR" size="20000"/>
        <column name="file_name" type="VARCHAR" size="500"/>
        <column name="file_rows_count" type="INTEGER"/>
        <column name="imported_rows_count" type="INTEGER"/>
        <column name="created_at" type="DATE" required="true"/>
    </table>

</database>
