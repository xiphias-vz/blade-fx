<?xml version="1.0"?>
<database xmlns="spryker:schema-01"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          name="zed"
          namespace="Orm\Zed\FactFinderExport\Persistence"
          package="src.Orm.Zed.FactFinderExport.Persistence"
          xsi:schemaLocation="spryker:schema-01 https://static.spryker.com/schema-01.xsd">

    <table name="pyz_fact_finder_event">
        <column name="id_fact_finder_event" required="true" type="BIGINT" autoIncrement="true" primaryKey="true"/>
        <column name="fk_product_abstract" type="INTEGER" required="true"/>
        <column name="event_name" type="VARCHAR" size="100"/>
        <column name="class_name" type="VARCHAR" size="100"/>
        <column name="event_data" type="VARCHAR" size="1000"/>
        <column name="created_at" required="true" type="TIMESTAMP"/>
    </table>

</database>
