<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          name="zed"
          xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema-01.xsd"
          namespace="Orm\Zed\AlternativeEan\Persistence"
          package="src.Orm.Zed.AlternativeEan.Persistence">

    <table name="pyz_alternative_ean">
        <column name="id_alternative_ean" required="true" type="BIGINT" autoIncrement="true" primaryKey="true"/>
        <column name="sap_number" type="VARCHAR"/>
        <column name="product_group_id" type="VARCHAR"/>
        <column name="name" type="VARCHAR"/>
        <column name="ean" type="VARCHAR"/>
        <column name="is_active" type="BOOLEAN"/>
        <column name="is_sales_unit" type="BOOLEAN"/>
        <column name="unit_of_measure_sap" type="VARCHAR"/>

        <index>
            <index-column name="sap_number"/>
        </index>

    </table>

</database>
