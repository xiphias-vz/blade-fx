<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="DeliveryOrderCollection">
        <property name="readyForDeliveryOrders" type="DeliveryOrder[]" singular="readyForDeliveryOrder"/>
        <property name="alreadyAssignedOrders" type="DeliveryOrder[]" singular="alreadyAssignedOrder"/>
        <property name="deliveredOrders" type="DeliveryOrder[]" singular="deliveredOrder"/>
        <property name="notDeliveredOrders" type="DeliveryOrder[]" singular="notDeliveredOrder"/>
    </transfer>

    <transfer name="DeliveryOrder">
        <property name="idSalesOrder" type="int"/>
        <property name="orderReference" type="string"/>
        <property name="deliveryAddress" type="Address"/>
        <property name="pickingBags" type="int"/>
        <property name="isStarted" type="bool"/>
        <property name="isDelivered" type="bool"/>
        <property name="deliveredAt" type="string"/>
        <property name="isCancelled" type="bool"/>
        <property name="assignedUser" type="User"/>
        <property name="requestedDeliveryDate" type="string"/>
        <property name="deliveryMinimumAge" type="int"/>
    </transfer>

    <transfer name="DeliveryTimeSlot">
        <property name="date" type="string"/>
        <property name="timeMin" type="string"/>
        <property name="timeMax" type="string"/>
    </transfer>

</transfers>
