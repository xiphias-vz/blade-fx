{% extends template('page-layout-main') %}

{% define data = {
    pickingZoneSelectionForm: pickingZoneSelectionForm,
} %}

{% block header %}
    {% include organism('header') with {
        data: {
            headerLocationTitle: 'order.overview.select.zone' | trans,
            withoutLogo: true,
            titleColor: 'darkerGreen'
        }
    } only %}
{% endblock %}

{% block content %}
    {% include molecule('select-picking-zone-form', 'Picker') with {
        data: {
            form: data.pickingZoneSelectionForm,
        },
    } only %}
{% endblock %}
