{% extends template('page-layout-main') %}

{% define data = {
    activities:activities,
} %}


 {% block header %}
     {% embed organism('header') only %}
         {% block selectPickingZone %}{% endblock %}
     {% endembed %}
 {% endblock %}


{% block content %}
    <div class="form btn-holder">
        {% for activity in data.activities %}
                <a class="button btn-section" href="{{ activity.path }}">{{ activity.name | trans }}</a>
        {% endfor %}
    </div>
{% endblock content %}


